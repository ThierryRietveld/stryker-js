  "version": "0.2.0",
  "configurations": [
      {
        "command": "cd packages/cucumber-runner && npm run stryker",
        "name": "Run npm start on cucumber",
        "request": "launch",
        "type": "node-terminal"
      },
      {
        "command": "cd ../stryker5-4-1/packages/core && node ../../../group-mutants/packages/core/bin/stryker run",
        "name": "Run npm start on Stryker5-4-1",
        "request": "launch",
        "type": "node-terminal"
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to Port",
        "address": "localhost",
        "port": 9229,
        "skipFiles": [
          "<node_internals>/**"
        ]
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Mocha Tests Mytest integration",
        "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
        "args": [
            "-u",
            "tdd",
            "--timeout",
            "999999",
            "${workspaceFolder}\\packages\\typescript-group-checker\\dist\\test\\integration\\"
        ],
        "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "command": "cd packages/typescript-checker && npm run test:integration",
      "name": "Run npm test:integration",
      "request": "launch",
      "type": "node-terminal"
    },
  ]
}
